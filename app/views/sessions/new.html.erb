<h1>Masuk admin</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for :session, url: sessions_path do |f| %>
      <%= f.label :Email %>
      <%= f.text_field :email %>

      <%= f.label :Password %>
      <%= f.password_field :password %>
      <%= f.submit "Masuk", class: "btn btn-large btn-primary" %>

    <% end %>
  </div>
</div>